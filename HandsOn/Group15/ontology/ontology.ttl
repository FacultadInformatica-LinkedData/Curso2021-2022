@base <http://parquimetrosdemadrid.com/ontology/parquimetros#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix : <http://parquimetrosdemadrid.com/ontology/parquimetros#>.


## Object 
#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasLocatedDistrict
:hasLocatedDistrict rdf:type owl:Objecterty;
                    rdfs:domain :ParkingMeter;
                    rdfs:range :District.
#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasLocatedNeighborhood
:hasLocatedNeighborhood rdf:type owl:Objecterty;
                        rdfs:domain :ParkingMeter;
                        rdfs:range :Neighborhood.
#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasLocatedVia
:hasLocatedVia rdf:type owl:Objecterty;
               rdfs:domain :ParkingMeter;
               rdfs:range :Via.

## Data
#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasLicensePlate
:hasLicensePlate rdf:type owl:Datatypeerty;
                 rdfs:domain :ParkingMeter;
                 rdfs:range xsd:string.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasGis_X
:hasGis_X  rdf:type owl:Datatypeerty;
           rdfs:domain :ParkingMeter;
           rdfs:range xsd:float.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasGis_Y
:hasGis_Y  rdf:type owl:Datatypeerty;
           rdfs:domain :ParkingMeter;
           rdfs:range xsd:float.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasReleaseDate
:hasReleaseDate rdf:type owl:Datatypeerty;
                rdfs:domain :ParkingMeter;
                rdfs:range xsd:date.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasDistrictName
:hasDistrictName rdf:type owl:Datatypeerty;
                 rdfs:domain :District;
                 rdfs:range xsd:string.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasNeighborhoodName
:hasNeighborhoodName rdf:type owl:Datatypeerty;
                     rdfs:domain :Neighborhood;
                     rdfs:range xsd:string.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasViaName
:hasViaName rdf:type owl:Datatypeerty;
            rdfs:domain :Via;
            rdfs:range xsd:string.

#https://http://parquimetrosdemadrid.com/ontology/parquimetros#hasViaNumber
:hasViaNumber rdf:type owl:Datatypeerty;
              rdfs:domain :Via;
              rdfs:range xsd:string.


## Classes 

# http://parquimetrosdemadrid.com/ontology/parquimetros#ParkingMeter
:ParkingMeter rdf:type owl:Class .

# http://parquimetrosdemadrid.com/parquimetros#District
:District rdf:type owl:Class.

# http://parquimetrosdemadrid.com/parquimetros#Neighborhood
:Neighborhood rdf:type owl:Class .

# http://parquimetrosdemadrid.com/parquimetros#Via
:Via rdf:type owl:Class .
